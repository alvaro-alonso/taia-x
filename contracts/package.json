{
  "name": "taia-x-contracts",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Taia-x Contracts",
  "dependencies": {
    "@taquito/signer": "^10.1.2",
    "@taquito/taquito": "^10.1.2",
    "dotenv": "^10.0.0"
  },
  "scripts": {
    "start-sandbox": "docker run --rm --name taia-x-sandbox --detach -p 20000:20000 tqtezos/flextesa:20210514 flobox start",
    "stop-sandbox": "docker kill taia-x-sandbox",
    "sandbox-info": "docker run --rm tqtezos/flextesa:20210514 flobox info",
    "compile": "docker run --rm -v \"%CD%\":/cd -w /cd ligolang/ligo:0.29.0 compile-contract ./contracts/src/fa2_nft_asset.mligo nft_asset_main > ./contracts/out/fa2_nft_asset.tz",
    "deploy:sandbox": "node scripts/deploy sandbox",
    "deploy:testnet": "node scripts/deploy testnet"
  }
}
